  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/server/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "generateGQLHooks": "graphql-codegen --config codegen.yml",
    "generateGQLHooks:watch": "yarn run generateGQLHooks --watch",
    "start:dev:with_generate_hooks": "concurrently \"yarn run start:dev\" \"yarn run generateGQLHooks:watch\""
  },
